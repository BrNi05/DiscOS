{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Start DiscOS (Dev)",
      "type": "shell",
      "command": "npm.cmd",
      "args": ["run", "dev"],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": "build",
      "problemMatcher": []
    },
    {
      "label": "Start DiscOS (Prod)",
      "type": "shell",
      "command": "npm.cmd",
      "args": ["run", "start"],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": "build",
      "problemMatcher": []
    },
    {
      "label": "Setup DiscOS",
      "type": "shell",
      "command": "npm.cmd",
      "args": ["run", "setup"],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": "none",
      "problemMatcher": []
    }
  ]
}
